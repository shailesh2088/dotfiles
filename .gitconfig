[user]
	name = Shailesh Samudrala
	email = <>
	[branch]
	  autosetuprebase = always

	[push]
	  default = upstream

	[core]
	  excludesfile = ~/.gitexcludes
	  editor = /usr/bin/vim

	[fetch]
	  recurseSubmodules = true

	[color]
	  diff = auto
	  status = auto
	  branch = auto
	  interactive = auto
	  ui = true

	[alias]
	  st = status
	  c = commit
	  a = add
	  sn = status --untracked=no
	  co = checkout
	  wc = whatchanged
	  l = log
	  b = branch
	  br = branch
	  sub = submodule
	  sm = submodule
	  cp = cherry-pick
	  last = "!git log | head -n 1 | cut -c 8- | xargs echo -n | pbcopy"
	  bbad = bisect bad
	  bgood = bisect good
      diffc = diff --cached
      prettylog = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
